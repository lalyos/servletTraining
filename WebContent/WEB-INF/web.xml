  <servlet>
    <description></description>
    <display-name>gohomeServlet</display-name>
    <servlet-name>gohomeServlet</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <description>message to display</description>
      <param-name>message</param-name>
      <param-value>Time left till end of day</param-value>
    </init-param>
    <init-param>
      <description>The time in yyyy-MM-dd HH:mm format</description>
      <param-name>breakTime</param-name>
      <param-value>2012-09-10 16:50</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <servlet-name>lastCoffee</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <param-name>message</param-name>
      <param-value>Time left till last coffee break</param-value>
    </init-param>
    <init-param>
      <param-name>breakTime</param-name>
      <param-value>XXXX2012-09-10 15:45</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <servlet-name>lastCoffeeLate</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <param-name>message</param-name>
      <param-value>You are late by</param-value>
    </init-param>
    <init-param>
      <param-name>breakTime</param-name>
      <param-value>2012-09-10 16:00</param-value>
    </init-param>
    <init-param>
      <param-name>isLate</param-name>
      <param-value>true</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>gohomeServlet</servlet-name>
