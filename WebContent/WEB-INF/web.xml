    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <description>message to display</description>
      <param-name>message</param-name>
      <param-value>Time left till end of day</param-value>
    </init-param>
    <init-param>
      <description>The time in yyyy-MM-dd HH:mm format</description>
